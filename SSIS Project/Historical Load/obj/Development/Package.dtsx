                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	ROW_NUMBER() over(order by(select null)) as SK_CompanyID,
	CIK as CompanyID,
	ST_NAME as Status,
	CompanyName as Name,
	IN_NAME as Industry,
	case when SPrating = 'AAA' 
			or SPrating = 'AA'
			or SPrating = 'AA+'
			or SPrating = 'AA-' 
			or SPrating = 'A'
			or SPrating = 'A+' 
