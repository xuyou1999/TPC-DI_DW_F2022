			or SPrating = 'CCC-' 
			or SPrating = 'CC'
			or SPrating = 'C' 
			or SPrating = 'D' 
			then SPrating
		else null
	end
		as SPrating,
	cast((case
		when (SPrating = 'AAA' 
			or SPrating = 'AA'
			or SPrating = 'AA+'
			or SPrating = 'AA-' 
			or SPrating = 'A'
			or SPrating = 'A+' 
