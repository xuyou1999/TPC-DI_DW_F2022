	DilutedEPS AS FI_DILUT_EPS, 
	Margin AS FI_MARGIN, 
	Inventory AS FI_INVENTORY, 
	Assets AS FI_ASSETS, 
	Liabilities AS FI_LIABILITY, 
	ShOut AS FI_OUT_BASIC, 
	DilutedShOut AS FI_OUT_DILUT
	FROM tpc_di_stage.dbo.FINWIRE_FIN
