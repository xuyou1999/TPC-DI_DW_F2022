(
	SELECT 
	CAST ( 
		CONCAT ( 
			SUBSTRING( PTS, 0, 5 ), '-', 
			SUBSTRING( PTS, 5, 2 ) , '-', 
			SUBSTRING( PTS, 7, 2 )
		) AS DATE
	) AS PTS, 
	CASE 
		WHEN ISNUMERIC( CoNameOrCIK ) = 1 
		THEN CAST( CoNameOrCIK AS INT ) 
		ELSE NULL 
	END AS CIK,
	CASE 
		WHEN ISNUMERIC( CoNameOrCIK ) = 0 
		THEN CoNameOrCIK 
		ELSE NULL 
